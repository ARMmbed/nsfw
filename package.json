{
  "name": "nsfw",
  "version": "1.2.7",
  "description": "A simple file watcher for Node",
  "main": "lib/src/index.js",
  "scripts": {
    "compile": "babel --sourceMaps --presets es2015 -d ./lib/spec ./js/spec && babel --sourceMaps --presets es2015 -d ./lib/src ./js/src",
    "lint": "eslint js/src js/spec",
    "prepublish": "babel --presets es2015 -d ./lib/src ./js/src",
    "test": "yarn lint && yarn compile && mocha --expose-gc lib/spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axosoft/node-simple-file-watcher.git"
  },
  "author": "Axosoft, LLC",
  "types": "index.d.ts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/axosoft/node-simple-file-watcher/issues"
  },
  "files": [
    "index.d.ts",
    "lib",
    "src",
    "includes",
    "binding.gyp"
  ],
  "homepage": "https://github.com/axosoft/node-simple-file-watcher",
  "dependencies": {
    "node-addon-api": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "eslint": "^6.8.0",
    "executive": "^1.6.3",
    "fs-extra": "^7.0.0",
    "mocha": "^7.1.1"
  },
  "keywords": [
    "FileWatcher",
    "files",
    "watch",
    "filewatch",
    "file",
    "inotify",
    "fsevents"
  ]
}
